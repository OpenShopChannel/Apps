        },
        {
            "treatment": "contents.delete",
            "arguments": [
                "apps/homebrew_browser/ShopChannel.ogg"
            ]
        },
        {
            "treatment": "contents.move",
            "arguments": [
                "Readme.txt",
                "apps/homebrew_browser/Readme.txt"
            ]
        }
    ]
}